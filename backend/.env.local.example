# Arquivo de exemplo para desenvolvimento
# Copie para .env.local e preencha com suas chaves

# Ambiente
ENVIRONMENT=development

# Supabase
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# Stripe
STRIPE_SECRET_KEY=sk_test_your-stripe-secret-key
STRIPE_WEBHOOK_SECRET=whsec_your-webhook-secret

# Price IDs (Stripe)
STRIPE_PRICE_ID_PRO_MONTHLY=price_your-pro-monthly-id

# Frontend
FRONTEND_CHECKOUT_RETURN_URL=http://localhost:3000/app

# APIs
GOOGLE_API_KEY=AIzaSyYour-Google-API-Key
GROQ_API_KEY=gsk_your-groq-api-key

# Debug
DEBUG_API_SECRET=vant_debug_2026_secure_key

# Rate Limiting (opcional - usa defaults se não definido)
RATE_LIMIT_ANALYZE_LITE=5/minute
RATE_LIMIT_ANALYZE_PAID=10/minute
RATE_LIMIT_INTERVIEW=10/minute

# Timeouts (opcional - usa defaults se não definido)
REQUEST_TIMEOUT_GLOBAL=180
REQUEST_TIMEOUT_INDIVIDUAL=120

# File Sizes (opcional - usa defaults se não definido)
MAX_AUDIO_SIZE_MB=10
MAX_PDF_SIZE_MB=5
MAX_FILE_SIZE_MB=15
